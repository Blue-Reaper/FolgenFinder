{
  "manifest_version": 2,
  "name": "FolgenFinder",
  "description": "Always be up to date with your favourite show and be notified of new episodes.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "FolgenFinder"
  },
  "options_ui": {
    "page": "options/index.html"
  }
}